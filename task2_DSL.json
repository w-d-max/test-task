{
  "dashboard": {
    "layout": {
      "desktop": {
        "columns": 12,
        "gap:": 10,
        "cellHeight": "auto"
      },
      "mobile": {
        "columns": 1,
        "gap": 20,
        "cellHeight": 200
      }
    },
    "charts": [
      {
        "title": "Chart1",
        "type": "Linear",
        "colors": ["#342443", "#45df43", "#ff0000"],
        "metrics": ["metricName1", "metricName2", "metricName3"],
        "layout": {
